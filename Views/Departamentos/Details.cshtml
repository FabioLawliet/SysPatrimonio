@model SysPatrimonio.Models.DtoDepartamento

@{
    ViewData["Title"] = "Details";
}

<h1>Departamentos</h1>
<h3>Detalhes</h3>
<div>
    <hr />
    <dl class="row rowForm">
        <dt class = "col-sm-2">
            Nome
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.nomedepartamento)
        </dd>
        <dt class = "col-sm-2">
            Descrição
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.descricaodepartamento)
        </dd>
        <dt class = "col-sm-2">
            Nome Local
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.nomelocal)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.id" class="btn btnCad btn-primary">Editar</a>
    <a asp-action="Index" class="btn btnCad btn-primary">Voltar</a>
</div>
