@model SysPatrimonio.Models.DbLocal

@{
    ViewData["Title"] = "Details";
}

<h1>Locais</h1>
<h3>Detalhes</h3>
<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Nome do Local
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.nomelocal)
        </dd>
        <dt class = "col-sm-2">
            Descrição
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.descricaolocal)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.id" class="btn btn-primary">Editar</a> 
    <a asp-action="Index" class="btn btn-primary">Voltar</a>
</div>
